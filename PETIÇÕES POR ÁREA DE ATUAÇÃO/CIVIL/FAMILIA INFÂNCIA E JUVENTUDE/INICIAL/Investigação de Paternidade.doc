\par {\pntext\pard\plain\f3\cgrid \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard \qj\fi-284\li284\sb120\sl-280\slmult0\nowidctlpar\widctlpar\tx567\tx1701\tx3402\tx5103\tx6804{\*\pn \pnlvlblt\ilvl0\ls1\pnrnot0\pnf3\pnstart1\pnindent284\pnhang{\pntxtb \'b7}}
